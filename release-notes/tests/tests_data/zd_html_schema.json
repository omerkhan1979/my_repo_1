{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "date": {
        "type": "string",
        "pattern": "^[0-9]{2} [A-Za-z]{3}, [0-9]{4}$"
      },
      "fixed_bugs": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "improvements": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "required": ["date", "fixed_bugs", "improvements"]
  }
}
